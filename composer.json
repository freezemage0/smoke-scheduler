{
  "name": "freezemage0/smoke-scheduler",
  "description": "Smoke scheduler",
  "type": "library",
  "license": "proprietary",
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Freezemage\\Smoke\\": "src/"
    }
  },
  "require": {
    "php": "^7.2",
    "ext-sockets": "*",
    "ext-pcntl": "*",
    "freezemage0/config": "^2",
    "ext-json": "*",
    "phing/phing": "*"
  },
  "scripts": {
    "build-phar": "php vendor/bin/phing -f build.xml",
    "move-phar": "mkdir -p /var/lib/smoke-scheduler/ && cp build/main.phar /var/lib/smoke-scheduler/"
  }
}
